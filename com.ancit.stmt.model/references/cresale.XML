<!-- XML tags to create Sales Voucher -->
<!-- It also creates the following -->
<!-- A Stock Group named "Spares" -->
<!-- A Stock Item  named "My Item1" -->
<!-- A Ledger named "Cash" if it does not exist -->
<!-- A Ledger named "Sale of Spares @4%" -->
<!-- A Ledger named "VAT Output @4%" -->

<ENVELOPE>
<HEADER>
<TALLYREQUEST>Import Data</TALLYREQUEST>
</HEADER>
<BODY>
<IMPORTDATA>
<REQUESTDESC>
<REPORTNAME>All Masters</REPORTNAME>
</REQUESTDESC>
<REQUESTDATA>

<!-- Create UNIT named "Nos" -->
<TALLYMESSAGE xmlns:UDF="TallyUDF">
 <UNIT NAME="Nos" ACTION="CREATE">
  <NAME>Nos</NAME>
  <ISSIMPLEUNIT>Yes</ISSIMPLEUNIT>
  <FORPAYROLL>No</FORPAYROLL>
 </UNIT>
</TALLYMESSAGE>

<!-- Create Stock Group named "Spares" -->
<TALLYMESSAGE xmlns:UDF="TallyUDF">
<STOCKGROUP NAME="SPARES" ACTION="Create">
<NAME.LIST>
<NAME>SPARES</NAME>
</NAME.LIST>
<PARENT/>
<ISADDABLE>Yes</ISADDABLE>
</STOCKGROUP>
</TALLYMESSAGE>

<!-- Create Stock Item named "My Item1" -->
<TALLYMESSAGE xmlns:UDF="TallyUDF">
<STOCKITEM NAME="MY ITEM1" ACTION="Create">
<NAME.LIST>
<NAME>MY ITEM1</NAME>
</NAME.LIST>
