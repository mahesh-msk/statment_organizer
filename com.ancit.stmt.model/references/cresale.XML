</STANDARDCOSTLIST.LIST>
<REORDERBASE>0.000</REORDERBASE>
<MINIMUMORDERBASE>0.000</MINIMUMORDERBASE>
</STOCKITEM>
</TALLYMESSAGE>

<!-- Create Ledger named "Cash" if it does not exist -->
<TALLYMESSAGE xmlns:UDF="TallyUDF">
<LEDGER NAME="CASH" ACTION="Create">
<NAME.LIST>
<NAME>CASH</NAME>
</NAME.LIST>
<PARENT>Cash-in-hand</PARENT>
<ISBILLWISEON>No</ISBILLWISEON>
<AFFECTSSTOCK>No</AFFECTSSTOCK>
<OPENINGBALANCE>0</OPENINGBALANCE>
<USEFORVAT>No </USEFORVAT>
<TAXCLASSIFICATIONNAME/>
<TAXTYPE/>
<RATEOFTAXCALCULATION/>
</LEDGER>
</TALLYMESSAGE>

<!-- Create Ledger named "Sale of Spares @4%" -->
<TALLYMESSAGE xmlns:UDF="TallyUDF">
<LEDGER NAME="SALE OF SPARES @4%" ACTION="Create">
<NAME.LIST>
<NAME>SALE OF SPARES @4%</NAME>
</NAME.LIST>
<PARENT>Sales Accounts</PARENT>
<ISBILLWISEON>No</ISBILLWISEON>
<AFFECTSSTOCK>Yes</AFFECTSSTOCK>
<OPENINGBALANCE>0</OPENINGBALANCE>
<USEFORVAT>Yes</USEFORVAT>
<TAXCLASSIFICATIONNAME>Sales @ 4%</TAXCLASSIFICATIONNAME>
</LEDGER>
</TALLYMESSAGE>

<!-- Create Ledger named "VAT Output @4%" -->
<TALLYMESSAGE xmlns:UDF="TallyUDF">
<LEDGER NAME="VAT OUTPUT 4%" ACTION="Create">
<NAME.LIST>
<NAME>VAT OUTPUT 4%</NAME>
</NAME.LIST>
<PARENT>Duties &amp; Taxes</PARENT>
